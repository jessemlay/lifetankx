Attribute VB_Name = "mASCII"
Option Explicit
    Dim arrChr(1 To 255)
    Public loadedArr As Boolean
    

Public Function g_Chr(lChrID As Integer) As String
    If loadedArr = False Then
        LoadArrayASCII
    End If
    
    If lChrID < 256 And lChrID > 0 Then
        g_Chr = arrChr(lChrID)
    Else
        g_Chr = 0
    End If

End Function

Public Function g_Asc(strChr As String) As Integer
Dim i As Integer
Dim foundMatch As Integer
    foundMatch = -1

    If loadedArr = False Then
        LoadArrayASCII
    End If
    
    For i = 1 To 255
        If arrChr(i) = strChr Then
            foundMatch = i
        End If
    Next i

    If foundMatch = -1 Then
        g_Asc = 0
    Else
        g_Asc = foundMatch
    End If
End Function

